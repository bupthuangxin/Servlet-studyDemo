<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GetInfoServlet</servlet-name>
    <servlet-class>com.huangxin.GetInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GetRequestHeader</servlet-name>
    <servlet-class>com.huangxin.GetRequestHeader</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MyInfoForm</servlet-name>
    <servlet-class>com.huangxin.MyInfoForm</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RegisterCl</servlet-name>
    <servlet-class>com.huangxin.RegisterCl</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>GetInfoServlet</servlet-name>
    <url-pattern>/GetInfoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetRequestHeader</servlet-name>
    <url-pattern>/GetRequestHeader</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyInfoForm</servlet-name>
    <url-pattern>/MyInfoForm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterCl</servlet-name>
    <url-pattern>/RegisterCl</url-pattern>
  </servlet-mapping>

</web-app>